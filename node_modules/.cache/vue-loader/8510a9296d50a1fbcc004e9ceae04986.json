n        <select id=\"rating\" v-model.number=\"rating\" required>\r\n          <option>10</option>\r\n          <option>9</option>\r\n          <option>8</option>\r\n          <option>7</option>\r\n          <option>6</option>\r\n          <option>5</option>\r\n          <option>4</option>\r\n          <option>3</option>\r\n          <option>2</option>\r\n          <option>1</option>\r\n        </select>\r\n      </p>\r\n\r\n      <p>\r\n        <input type=\"submit\" value=\"Submit\"/>\r\n      </p>\r\n    </form>\r\n\r\n    <ul id=\"example-1\">\r\n    <li v-for=\"item in reviews\" :key=\"item.review\">\r\n      {{item.name}} {{item.surname}} {{item.email}} {{item.rating}}/10 - {{item.review}}\r\n    </li>\r\n  </ul>\r\n\r\n</div>\r\n</template>\r\n  \r\n<script>\r\nexport default{\r\n  data: function () {\r\n      return {\r\n        name: null,\r\n        surname: null,\r\n        email: null,\r\n        review: null,\r\n        rating: null,\r\n        reviews: this.$store.state.reviews\r\n      };\r\n    },\r\n\r\n  methods: {\r\n      onSubmit: function () {\r\n        var productReview = {\r\n          name: this.name,\r\n          surname: this.surname,\r\n          email: this.email,\r\n          review: this.review,\r\n          rating: this.rating,\r\n        };\r\n\r\n        this.$store.commit(\"AGGIUNGI_RECENSIONE\", productReview); //invece di $emit si usa $store.commit\r\n        //this.$emit(\"review-submitted\", productReview);\r\n        this.name = null;\r\n        this.surname = null;\r\n        this.email= null;\r\n        this.review = null;\r\n        this.rating = null;\r\n      },\r\n    },\r\n}\r\n</script>\r\n\r\n"]}]}